{
  "author": "Iorio Ivano",
  "name": "@priolo/jon-utils",
  "version": "0.3.1",
  "description": "generic utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priolo/jon-utils.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/priolo/jon-utils/issues"
  },
  "homepage": "https://github.com/priolo/jon-utils#readme",
  "main": "dist/index.js",
  "module": "dist/index.umd.cjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest --watchAll=false src/tests",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "pub": "npm publish --access public",
    "build:types": "tsc -m ESNext -t ESNext -d --isolatedModules false  --moduleResolution node --esModuleInterop --jsx react --allowJs --outFile dist/index.js src/index.js",
    "build2:types": "tsc -d --isolatedModules false --declaration --emitDeclarationOnly --outFile dist/index.d.ts --allowJs src/index.ts"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^29.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.18.6"
  },
  "volta": {
    "node": "18.6.0",
    "npm": "8.15.0"
  }
}